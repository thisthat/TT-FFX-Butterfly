<Window x:Class="TT_FFX_Butterfly.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="504" Width="404" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" MouseMove="Window_MouseMove">
    <Grid>
        <Image x:Name="img" Margin="0,-1,4,55" Source="img/Macalania_Woods_Central2.png" MouseDown="img_MouseDown" Stretch="None"/>
        <Button x:Name="btnLoadFile" Content="Load" HorizontalAlignment="Left" Margin="10,0,0,16" Width="71" Click="btnLoadFile_Click" Height="25" VerticalAlignment="Bottom"/>
        <Button x:Name="btnStart" Content="Start" Margin="118,0,140,16" Click="btnStart_Click" Height="25" VerticalAlignment="Bottom"/>
        <Button x:Name="btnSaveFile" Content="Save" Margin="0,0,21,16" Click="btnSaveFile_Click" HorizontalAlignment="Right" Width="71" Height="25" VerticalAlignment="Bottom"/>
        <Canvas x:Name="tela" HorizontalAlignment="Left" Height="309" VerticalAlignment="Top" Width="325"/>
    </Grid>
</Window>
